package com.xwtech.xwecp.log;


import org.apache.log4j.Logger;


public abstract class LogBean
{
	private static final Logger logger = Logger.getLogger(LogBean.class);
	
	protected static final String LI_ACCESS_LOG = "LI_ACCESS_LOG";
	
	protected static final String BOSS_ACCESS_LOG = "BOSS_ACCESS_LOG";
	
	protected String f_access_id;
	
	protected String f_access_time;
	
	protected String f_response_time;
	
	protected String f_is_error;
	
	protected String f_result_code;
	
	protected String f_error_code;
	
	protected String f_error_msg;
	
	protected String f_req_text;
	
	protected String f_res_text;
	
	protected String f_error_stack;
	
	public abstract String getType();
}
