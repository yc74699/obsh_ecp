package com.xwtech.xwecp.pojo;

import java.io.Serializable;

public class PkgExgSelectBean implements Serializable{
	private static final long serialVersionUID = 6121438239255456196L;
	private String channelNum;
	//地市编码
	private String arerNum;
	//转入套餐组别编码
	private String impGroupNum;
	//ECP系统定义的转入套餐编码
	private String impPkgNumEcp;
	//备注
	private String bz;
	public String getChannelNum() {
		return channelNum;
	}
	public void setChannelNum(String channelNum) {
		this.channelNum = channelNum;
	}
	public String getArerNum() {
		return arerNum;
	}
	public void setArerNum(String arerNum) {
		this.arerNum = arerNum;
	}
	public String getImpGroupNum() {
		return impGroupNum;
	}
	public void setImpGroupNum(String impGroupNum) {
		this.impGroupNum = impGroupNum;
	}
	public String getImpPkgNumEcp() {
		return impPkgNumEcp;
	}
	public void setImpPkgNumEcp(String impPkgNumEcp) {
		this.impPkgNumEcp = impPkgNumEcp;
	}
	public String getBz() {
		return bz;
	}
	public void setBz(String bz) {
		this.bz = bz;
	}
	
	
}
