package com.xwtech.xwecp.service.logic.pojo;


public class MpmusicBillDetail
{
	private String sourceTypeName;

	private String spCode;

	private String contentId;

	private String startTime;

	private double realInfoFee;

	private String freetimeItem;

	private long freetime;

	private String chargeTypeName;

	public void setSourceTypeName(String sourceTypeName)
	{
		this.sourceTypeName = sourceTypeName;
	}

	public String getSourceTypeName()
	{
		return this.sourceTypeName;
	}

	public void setSpCode(String spCode)
	{
		this.spCode = spCode;
	}

	public String getSpCode()
	{
		return this.spCode;
	}

	public void setContentId(String contentId)
	{
		this.contentId = contentId;
	}

	public String getContentId()
	{
		return this.contentId;
	}

	public void setStartTime(String startTime)
	{
		this.startTime = startTime;
	}

	public String getStartTime()
	{
		return this.startTime;
	}

	public void setRealInfoFee(double realInfoFee)
	{
		this.realInfoFee = realInfoFee;
	}

	public double getRealInfoFee()
	{
		return this.realInfoFee;
	}

	public void setFreetimeItem(String freetimeItem)
	{
		this.freetimeItem = freetimeItem;
	}

	public String getFreetimeItem()
	{
		return this.freetimeItem;
	}

	public void setFreetime(long freetime)
	{
		this.freetime = freetime;
	}

	public long getFreetime()
	{
		return this.freetime;
	}

	public void setChargeTypeName(String chargeTypeName)
	{
		this.chargeTypeName = chargeTypeName;
	}

	public String getChargeTypeName()
	{
		return this.chargeTypeName;
	}

