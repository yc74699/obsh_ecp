package com.xwtech.xwecp.pojo;

import java.io.Serializable;

public class PackageBean implements Serializable{
	//渠道编码
	private String channelNum;
	//套餐在ECP系统对应的编码
	private String pkgNum;
	//套餐名称
	private String pkgName;
	//BOSS侧的产品ID
	private String productIdBoss;
	//BOSS侧的套餐类别
	private String pkgTypeBoss;
	//BOSS侧的套餐编码
	private String pkgNumBoss;
	//备注说明
	private String bz;
	
	public String getChannelNum() {
		return channelNum;
	}
	public void setChannelNum(String channelNum) {
		this.channelNum = channelNum;
	}
	public String getPkgNum() {
		return pkgNum;
	}
	public void setPkgNum(String pkgNum) {
		this.pkgNum = pkgNum;
	}
	public String getPkgName() {
		return pkgName;
	}
	public void setPkgName(String pkgName) {
		this.pkgName = pkgName;
	}
	public String getProductIdBoss() {
		return productIdBoss;
	}
	public void setProductIdBoss(String productIdBoss) {
		this.productIdBoss = productIdBoss;
	}
	public String getPkgTypeBoss() {
		return pkgTypeBoss;
	}
	public void setPkgTypeBoss(String pkgTypeBoss) {
		this.pkgTypeBoss = pkgTypeBoss;
	}
	public String getPkgNumBoss() {
		return pkgNumBoss;
	}
	public void setPkgNumBoss(String pkgNumBoss) {
		this.pkgNumBoss = pkgNumBoss;
	}
	public String getBz() {
		return bz;
	}
	public void setBz(String bz) {
		this.bz = bz;
	}
	
	
}
